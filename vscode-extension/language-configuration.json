{
  "comments": {
    "lineComment": "//"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*\\b(page|container|form)\\b.*$",
    "decreaseIndentPattern": "^\\s*$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*\\b(page|container|form)\\b",
      "end": "^\\s*$"
    }
  },
  "wordPattern": "[a-zA-Z][a-zA-Z0-9_]*"
} 